<h3>Your Food Inventory</h3>
<ul class='itemsIndex'>
  <% current_user.items.each do |item| %>
  	<% if !item.new_record? %>
      <li><%= item.id.to_s %> | <%= item.item_kind_id.to_s %> <%= item.item_kind.name unless item.item_kind.nil? %> expires at
          <span class="date"><%= item.expiration.to_s %></span>.
      </li>
    <% end %>
  <% end %>
</ul>