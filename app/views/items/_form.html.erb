<%= form_for item, :remote => true do |f| %>
  <%= f.text_field :item_kind_name, :data => {:autocomplete_source => item_kinds_path }, :placeholder => "Add your item here" %>
  <%= f.select :storage, options_for_select(["Refridgerator", "Pantry", "Freezer"]) %>
  <%= f.submit :class => "btn btn-primary" %>
<% end %>

